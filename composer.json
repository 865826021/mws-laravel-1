{
    "name": "luigel/laravel5-amazon-mws",
    "description": "Use Amazon's MWS web services with Laravel 5.x. Based on creacoon/amazon-mws-laravel package and modified to make it compatible with latest Laravel releases (+ bugfixes).",
    "license": "Apache-2.0",
    "keywords": ["API", "Amazon", "PHP", "MWS", "Laravel"],
    "authors": [
        {
            "name": "Rigel Kent Carbonel",
            "email": "rigel20.kent@gmail.com"
        },
        {
          "name": "Kenneth Diputado",
          "email": "kennethdiputado@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "ext-curl": "*",
        "illuminate/support": "5.*",
        "sonnenglas/amazon-mws-laravel": "stable"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*"
    },
    "autoload": {
        "psr-4": {
            "Luigel\\AmazonMws\\": "src"
        }
    },
    "minimum-stability": "stable"
}
